{
	"$schema": "https://raw.githubusercontent.com/Bedrock-OSS/regolith-schemas/main/config/v1.4.json",
	"author": "Your name",
	"name": "Project name",
	"packs": {
		"behaviorPack": "./packs/BP",
		"resourcePack": "./packs/RP"
	},
	"regolith": {
		"dataPath": "./packs/data",
		"filterDefinitions": {
			"hello-nested-path-filter": {
				"url": "github.com/Bedrock-OSS/regolith-test-filters-manifest",
				"version": "1.0.1"
			}
		},
		"formatVersion": "1.4.0",
		"profiles": {
			"default": {
				"export": {
					"target": "local"
				},
				"filters": [
					{
						"filter": "hello-nested-path-filter"
					}
				]
			}
		}
	}
}