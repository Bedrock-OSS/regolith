{
	"$schema": "https://raw.githubusercontent.com/Bedrock-OSS/regolith-schemas/main/config/v1.2.json",
	"author": "Bedrock-OSS",
	"name": "regolith_test_project",
	"packs": {
		"behaviorPack": "./packs/BP",
		"resourcePack": "./packs/RP"
	},
	"regolith": {
		"dataPath": "./packs/data",
		"filterDefinitions": {
			"my_release_filter": {
				"url": "github.com/akashic-records-of-the-abyss/basic_regolith",
				"version": "1.0.0"
			},
			"nested_filter": {
				"url": "github.com/akashic-records-of-the-abyss/basic_regolith",
				"version": "1.0.0"
			}
		},
		"profiles": {
			"default": {
				"export": {
					"target": "local"
				},
				"filters": [
					{
						"filter": "my_release_filter"
					},
					{
						"filter": "nested_filter"
					}
				]
			}
		}
	}
}